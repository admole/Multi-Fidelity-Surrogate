FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/initialConditions"

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $pressure;

boundaryField
{
    INLET
    {
        type            zeroGradient;
    }

    OUTLET
    {
        type            fixedValue;
        value           $internalField;
    }

    WALL
    {
        type            zeroGradient;
    }

    CUBE1
    {
        type            zeroGradient;
    }

    CUBE2
    {
        type            zeroGradient;
    }


    #include "include/topLeftRightPatches"
}

