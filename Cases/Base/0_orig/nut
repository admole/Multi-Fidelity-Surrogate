FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    WALL
    {
        type            nutkWallFunction;
        value           uniform 0;
    }

    CUBE1
    {
        type            nutkWallFunction;
        value           uniform 0;
    }

    CUBE2
    {
        type            nutkWallFunction;
        value           uniform 0;
    }

    "(TOP|INLET|OUTLET)"
    {
        type            calculated;
        value           uniform 0;
    }

    "(LEFT|RIGHT)"
    {
        type            cyclic;
    }
}


