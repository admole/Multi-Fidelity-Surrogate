FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/initialConditions"

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $turbulentKE;

boundaryField
{
    #include "include/fixedInlet"

    OUTLET
    {
        type            inletOutlet; 
        inletValue      $internalField;
        value           $internalField;
    }
    
    WALL
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    CUBE1
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    CUBE2
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    #include "include/topLeftRightPatches"
}


