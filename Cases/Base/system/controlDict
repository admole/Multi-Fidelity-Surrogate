FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs
(
//    "libMyCorrection.so"
);

application     pimpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         25;

adjustTimeStep  no;

deltaT          0.001;

writeControl     timeStep;

writeInterval   500;

purgeWrite      1;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;


functions
{
    turbulenceFields
    {
        type            turbulenceFields;
        libs            ("libfieldFunctionObjects.so");
        writeControl    writeTime;
        fields          (R nut nuTilda k epsilon omega);
    }

    #include "averaging"
    #include "surfaces"
    #include "lines"
    #include "skinFriction"
    #include "yPlus"
    #include "forceCoeffs"

    #include "fileUpdate"
}

