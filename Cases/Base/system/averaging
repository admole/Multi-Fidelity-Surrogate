FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      lines;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

    fieldAverage
    {
        type            fieldAverage;
        libs ("libfieldFunctionObjects.so");

        writeControl        writeTime;

        fields
        (
            U
            {
                mean            on;
                prime2Mean      on;
                base            time;

                windowType      approximate;
                window          10;
                allowRestart    false;
            }

            p
            {
                mean            on;
                prime2Mean      off;
                base            time;

                windowType      approximate;
                window          10;
                allowRestart    false;
            }

            k
            {
                mean            on;
                prime2Mean      off;
                base            time;

                windowType      approximate;
                window          10;
                allowRestart    false;
            }

            nut
            {
                mean            on;
                prime2Mean      off;
                base            time;

                windowType      approximate;
                window          10;
                allowRestart    false;
            }
        );
    }


